{
    "pdf": {
        "content": [{
                "files": [
                    "**/*.md",
                    "**/**.yml"
                ],
                "exclude": [
                    "**/toc.md",
                    "**/toc.yml",
                    "**/obj/**",
                    "**/includes/**",
                    "**/_themes.pdf/**",
                    "README.md",
                    "LICENSE",
                    "LICENSE-CODE",
                    "**/archive/**",
                    "contribute/templates/global/*.md",
                    "**/ftdocs_site/**",
                    "**/_tools/**",
                    "**/test-data/**",
                    "azure-pipelines.yml",
                    "azure-pipelines-pr.yml"
                ]
            },
            {
                "files": "pdf/toc.yml"
            }
        ],
        "resource": [{
            "files": [
                "**/*.png",
                "**/*.jpg",
                "**/*.gif",
                "**/*.pdf",
                "**/*.pptx",
                "favicon.ico"
            ],
            "exclude": [
                "**/obj/**",
                "**/includes/**"
            ]
        }],
        "overwrite": [],
        "name": "FastTrack",
        "tocTitle": "FastTrack Documentation",
        "coverTitle": "FastTrack Documentation",
        "template": ["pdf.default"],
        "outline": "WkDefaultOutline",
        "keepRawFiles": "false",
        "dest": "ftdocs_site/pdf",
        "noStdin": "true"
    }
}